      {
        "id": "54002b92-cedf-41db-b522-7d9d3298e804",
        "position": 4,
        "text": "Відповіді 1, 2 і 3.",
        "isCorrect": false
      }
    ]
  },
  {
    "id": "542baabf-7c6f-4c93-8c89-66c5b4de7c9a",
    "sectionId": "null",
