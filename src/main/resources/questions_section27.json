      {
        "id": "7fb5667c-9812-413f-b0b0-8b4fe19407df",
        "position": 1,
        "text": "Не повинні.",
        "isCorrect": false
      },
      {
        "id": "ece47174-63ca-4902-999b-4591f6b9992f",
        "position": 2,
        "text": "Повинні завжди.",
        "isCorrect": true
