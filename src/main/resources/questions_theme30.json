      {
        "id": "d3b83af7-280d-414e-9548-7f6ca3dc114b",
        "position": 1,
        "text": "З боків.",
        "isCorrect": false
      },
      {
        "id": "d83b527e-a46f-4e7b-b9b2-833698a68607",
        "position": 2,
        "text": "Спереду і ззаду.",
        "isCorrect": false
      },
      {
        "id": "c4045aee-9712-4178-ab37-13c6bc3a176f",
        "position": 3,
        "text": "У передбачених для цього місцях.",
        "isCorrect": true
