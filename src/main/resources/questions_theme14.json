    "answerOptionList": [
      {
        "id": "6a24a7ad-a5a2-4a40-b091-f04b574e34de",
        "position": 1,
        "text": "Не може.",
        "isCorrect": false
      },
      {
        "id": "5b958686-fa9f-4dda-8a85-323319e3a1cb",
        "position": 2,
        "text": "Може за умови, що обгін буде завершено за 100 м до переїзду.",
        "isCorrect": true
      },
      {
        "id": "df4b4ab1-7baa-4595-b92f-33764ffe1459",
        "position": 3,
        "text": "Може в будь-якому випадку.",
        "isCorrect": false
      }
    ]
  },
  {
    "id": "3cbfe4fd-7ce4-4772-b9be-e71a71a75eac",
    "themeId": "7293dbd5-3fd5-48eb-93db-d53fd5f8eb1e",
    "sourceOrder": 5,
    "order": 5,
